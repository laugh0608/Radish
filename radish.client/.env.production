# 生产环境配置

# API 基础 URL（生产环境域名）
VITE_API_BASE_URL=https://your-domain.com

# 认证服务器 URL（生产环境域名）
VITE_AUTH_BASE_URL=https://your-domain.com

# 是否启用 Mock 数据
VITE_ENABLE_MOCK=false

# 是否启用调试模式
VITE_DEBUG=false

# SignalR Hub URL
VITE_SIGNALR_HUB_URL=https://your-domain.com

# 功能开关
VITE_FEATURE_DARK_MODE=true
VITE_FEATURE_I18N=true

# 论坛固定标签（逗号分隔）
VITE_FORUM_FIXED_TAGS=C#,.NET,Web开发,数据库,综合
