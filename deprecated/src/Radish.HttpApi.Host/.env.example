# Copy this file to `.env` in the same directory and fill the values.

# MongoDB connection strings
ConnectionStrings__Default=mongodb://
ConnectionStrings__Chrelyonly=mongodb://

# Allowed CORS origins (comma separated). Include both http and https for local dev.
# Example for Angular (4200) and React (5173):
App__CorsOrigins=http://localhost:4200,https://localhost:4200,http://localhost:5173,https://localhost:5173

# Optional: override Angular app url used in links (defaults to appsettings.json if not set)
# App__AngularUrl=http://localhost:4200

# Optional: redirect whitelist for Account endpoints
# App__RedirectAllowedUrls=http://localhost:4200,https://localhost:4200,http://localhost:5173,https://localhost:5173

# Optional: OpenIddict certificate pass phrase (dev only)
# AuthServer__CertificatePassPhrase=CHANGE_ME_IN_ENV

# 字符串加密密钥
# StringEncryption__DefaultPassPhrase=CHANGE_ME_IN_ENV
