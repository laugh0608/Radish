  // Redis 配置
  "Redis": {
    // 缓存开关，true 为使用 Redis，false 为使用内存缓存
    "Enable": true,
    // 示例：'127.0.0.1:6379,password=123456,allowAdmin=true'
    "ConnectionString": "api.chrelyonly.cn:16371,password=redis-public1,allowAdmin=true",
    // Redis 前缀
    "InstanceName": "Radish"
  },
  // 数据库配置，不区分单库多库
  // MainDb: 标识当前项目的主库，所对应的连接字符串的 Enabled 必须为 true
  // Log: 标识日志库，所对应的连接字符串的 Enabled 必须为 true
  "MainDb": "Main",
  // 当前项目的主库，所对应的连接字符串的 Enabled 必须为 true
  "Databases": [
    /**
     * 对应下边的 DbType
     * MySql = 0,
     * SqlServer = 1,
     * Sqlite = 2,
     * Oracle = 3,
     * PostgreSql = 4,
     * DaMeng = 5, // 达梦
     * Kdbndp = 6, // 金仓
     */
    {
      "ConnId": "Main",
      "DbType": 2,
      "Enabled": true,
      // Sqlite 只写数据库名就行，对应的，数据库文件要在项目根目录下
      "ConnectionString": "Radish.db"
    },
    {
      // 日志库连接固定名称，不要改,其他的可以改
      "ConnId": "Log",
      "DbType": 2,
      "Enabled": true,
      // 从库执行级别，越大越先执行
      "HitRate": 50,
      // Sqlite 只写数据库名就行，对应的，数据库文件要在项目根目录下
      "ConnectionString": "RadishLog.db"
    }
  ],
}
